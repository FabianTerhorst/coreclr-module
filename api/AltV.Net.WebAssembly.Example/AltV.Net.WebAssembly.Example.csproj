<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>AltV.Net.WebAssembly.Example</RootNamespace>
    <SignAssembly>true</SignAssembly>
    <NoStdLib>true</NoStdLib>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <ProjectReference Include="..\AltV.Net.Client\AltV.Net.Client.csproj" />
    <ProjectReference Include="..\AltV.Net.WebAssembly.Bindings\AltV.Net.WebAssembly.Bindings.csproj" />
  </ItemGroup>
</Project>
